{
  "name": "wharf-digital",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/wharf-digital",
  "projectType": "application",
  "tags": [],
  "// targets": "to see all targets run: nx show project wharf-digital --web",
  "targets": {
    "generate-sanity-types": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/wharf-digital-cms",
        "commands": [
          {
            "command": "npx sanity schema extract"
          },
          {
            "command": "npx sanity typegen generate"
          }
        ],
        "parallel": false
      }
    }
  }
}
